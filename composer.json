{
    "name": "da/stat-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2's bundle providing an architecture to make statistics. LOAD your data and RENDER it.",
    "keywords": ["stat", "statistics", "bundle", "da", "helper", "architecture"],
    "homepage": "http://github.com/Gnucki/DaStatBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Prelot",
            "email": "thomas.prelot@gmail.fr"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/dependency-injection" : ">=2.1",
        "symfony/config" : ">=2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "Da\\StatBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Da/StatBundle"
}